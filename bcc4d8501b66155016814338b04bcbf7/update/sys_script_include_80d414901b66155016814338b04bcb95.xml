        <name>Utils</name>
        <script><![CDATA[var Utils = {
	/**
	 * Retrieves the GlideRecord using the provided sysId and tableName
	 * Returns the GlideRecord if the record is found else returns null
	 * @param {string} sysId
	 * @param {string} tableName
	 * @returns {GlideRecord}
	 */
	getRecord: function(sysId, tableName) {
		var glideRecord = new GlideRecord(tableName);
		if(glideRecord.get(sysId)) {
			return glideRecord;
		} else {
			return null;
		}
	}
};]]></script>
